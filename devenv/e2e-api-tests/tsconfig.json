{
    "compilerOptions": {
      "moduleResolution": "node",
      "target": "es6",
      "lib": ["es6"],
      "module": "commonjs",
      "declaration": false,
      "allowSyntheticDefaultImports": true,
      "inlineSourceMap": false,
      "sourceMap": true,
      "noEmitOnError": false,
      "emitDecoratorMetadata": false,
      "experimentalDecorators": true,
      "noImplicitReturns": true,
      "noImplicitThis": false,
      "noImplicitUseStrict":false,
      "noImplicitAny": false,
      "noUnusedLocals": true
    },
    "include": [
      "*.ts",
      "**/*.ts"
    ]
}
