{
  "compilerOptions": {
    "declarationDir": "dist",
    "outDir": "compiled",
    "rootDirs": ["."],
    "paths": {
      "@grafana/data": ["."]
    },
    "incremental": true,
    "tsBuildInfoFile": "./tsconfig.tsbuildinfo"
  },
  "exclude": ["dist", "node_modules"],
  "extends": "@grafana/tsconfig",
  "include": [
    "src/**/*.ts*",
    "typings/jest",
    "../../public/app/types/jquery/*.ts",
    "../../public/app/types/*.d.ts",
    "../../public/app/types/sanitize-url.d.ts"
  ]
}
