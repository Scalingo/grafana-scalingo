{
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "name": "@grafana/e2e",
  "version": "8.5.3",
  "description": "Grafana End-to-End Test Library",
  "keywords": [
    "cli",
    "grafana",
    "e2e",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/grafana/grafana.git",
    "directory": "packages/grafana-e2e"
  },
  "main": "src/index.ts",
  "bin": {
    "grafana-e2e": "bin/grafana-e2e.js"
  },
  "scripts": {
    "build": "grafana-toolkit package:build --scope=e2e",
    "bundle": "rollup -c rollup.config.ts",
    "clean": "rimraf ./dist ./compiled",
    "docsExtract": "mkdir -p ../../reports/docs && api-extractor run 2>&1 | tee ../../reports/docs/$(basename $(pwd)).log",
    "open": "cypress open",
    "start": "cypress run --browser=chrome",
    "start-benchmark": "CYPRESS_NO_COMMAND_LOG=1 yarn start",
    "test": "pushd test && node ../dist/bin/grafana-e2e.js run",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "21.0.2",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@types/chrome-remote-interface": "0.31.4",
    "@types/lodash": "4.14.181",
    "@types/node": "16.11.26",
    "@types/uuid": "8.3.4",
    "rollup": "2.70.1",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-terser": "7.0.2",
    "webpack": "5.70.0"
  },
  "types": "src/index.ts",
  "dependencies": {
    "@babel/core": "7.17.8",
    "@babel/preset-env": "7.16.11",
    "@cypress/webpack-preprocessor": "5.11.1",
    "@grafana/e2e-selectors": "8.5.3",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@mochajs/json-file-reporter": "^1.2.0",
    "babel-loader": "8.2.4",
    "blink-diff": "1.0.13",
    "chrome-remote-interface": "0.31.2",
    "commander": "8.3.0",
    "cypress": "9.5.1",
    "cypress-file-upload": "5.0.8",
    "devtools-protocol": "0.0.982423",
    "execa": "5.1.1",
    "lodash": "4.17.21",
    "mocha": "9.2.2",
    "resolve-as-bin": "2.1.0",
    "rimraf": "3.0.2",
    "tracelib": "1.0.1",
    "ts-loader": "6.2.1",
    "tslib": "2.3.1",
    "typescript": "4.4.4",
    "uuid": "8.3.2",
    "yaml": "^1.8.3"
  }
}
